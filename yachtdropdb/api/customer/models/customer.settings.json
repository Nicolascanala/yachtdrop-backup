    "customer_email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "vessel": {
      "type": "string"
    },
    "customer_password": {
      "type": "password",
      "required": false
    },
    "customer_id": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "reset_password": {
      "type": "password"
    },
    "date_joined": {
      "type": "date"
    }
  }
